<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.todo_list">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_icon_todolist"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Todo_List">
        <activity
            android:name=".activity.Add_Task"
            android:exported="false" />
        <activity
            android:name=".activity.Task_Group"
            android:exported="false" />
        <activity
            android:name=".activity.MemberActivity"
            android:exported="false" />
        <activity
            android:name=".activity.Main__ToDo"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.Sign_Up"
            android:exported="false" />
        <activity
            android:name=".activity.Main_Login"
            android:exported="false" />
        <activity
            android:name=".activity.Login"
            android:exported="false" />
        <activity
            android:name=".activity.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".sevices.BroadRececiver"/>
        <service android:name=".sevices.NotifitionServices" android:enabled="true"/>
    </application>

</manifest>